<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

<item android:id="@android:id/background">
    <shape>
        <corners android:radius="1dip" />
        <gradient
                android:startColor="#414141"
                android:centerColor="#414141"
                android:centerY="0.75"
                android:endColor="#414141"
                android:angle="270"
        />
    </shape>
</item>

<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
            <corners android:radius="1dip" />
            <gradient
                    android:startColor="#2D5200"
                    android:centerColor="#2D5200"
                    android:centerY="0.75"
                    android:endColor="#2D5200"
                    android:angle="270"
            />
        </shape>
    </clip>
</item>
<item
    android:id="@android:id/progress"
>
    <clip>
        <shape>
            <corners
                android:radius="1dip" />
            <gradient
                android:startColor="#62B101"
                android:endColor="#62B101"
                android:angle="270" />
        </shape>
    </clip>
</item>

</layer-list>